<div class="page-title">
    <h1>Edit Review</h1>
    <p class="link-wrapper"><%= link_to "Cancel", review_path(@review), class: "button" %></p>
</div>
<div class='form'>
    <%= render 'form' %>
</div>

<% if flash[:myerrors] %>
    <div>
        <% flash[:myerrors].each do |error|  %>
            <%= error %> <br>
        <%end%>
    </div>
<%end%>
